{
  "name": "qusly",
  "version": "0.1.0",
  "description": "A full-featured, elegant FTP client.",
  "keywords": [
    "typescript",
    "react",
    "styled-components",
    "electron",
    "ftp",
    "sftp",
    "client",
    "qusly"
  ],
  "author": {
    "name": "Miko≈Çaj Palkiewicz",
    "email": "xnerhu@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qusly/qusly.git"
  },
  "bugs": {
    "url": "https://github.com/qusly/qusly#issues"
  },
  "main": "./build/main.bundle.js",
  "scripts": {
    "dev": "cross-env START=1 npm run watch",
    "watch": "cross-env ENV='dev' concurrently \"webpack\" \"webpack-dev-server --config webpack.config.renderer.js\" ",
    "build": "concurrently \"webpack --config webpack.config.renderer.js\" \"webpack\"",
    "start": "cross-env ENV='dev' electron .",
    "compile-win32": "npm run build && electron-builder -w -p always",
    "compile-darwin": "npm run build && electron-builder -m -p always",
    "compile-linux": "npm run build && electron-builder -l -p always",
    "lint": "eslint \"src/**/*.ts*\" \"src/**/*.tsx*\"",
    "lint-fix": "npm run lint -- --fix",
    "rebuild": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/runtime": "^7.8.7",
    "@hot-loader/react-dom": "^16.12.0",
    "@types/gsap": "1.20.2",
    "@types/keytar": "^4.4.2",
    "@types/node": "^13.7.7",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/styled-components": "5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "dotenv": "8.2.0",
    "electron": "8.0.3",
    "electron-builder": "22.4.0",
    "electron-rebuild": "^1.10.0",
    "electron-updater": "^4.2.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "file-loader": "^5.1.0",
    "gsap": "^3.2.4",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^3.2.0",
    "knex": "^0.20.11",
    "mobx": "^5.15.4",
    "mobx-react-lite": "^1.5.2",
    "prettier": "^1.19.1",
    "pretty-bytes": "^5.3.0",
    "qusly-core": "^2.5.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-hot-loader": "^4.12.19",
    "react-windows-controls": "^1.1.1",
    "rectangle-selection": "^2.5.0",
    "slash": "^3.0.0",
    "source-map-support": "^0.5.16",
    "styled-components": "^5.0.1",
    "terser": "^4.6.6",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "keytar": "^5.4.0",
    "sqlite3": "^4.1.1"
  }
}
