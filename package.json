{
  "name": "qusly",
  "version": "1.0.0",
  "description": "An FTP/FTPS/SFTP client with Material Design UI.",
  "keywords": [
    "typescript",
    "react",
    "styled-components",
    "electron",
    "fuse-box",
    "wexond",
    "ftp",
    "sftp",
    "client"
  ],
  "author": {
    "name": "Miko≈Çaj Palkiewicz",
    "email": "xnerhu@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xNerhu/ftp-client.git"
  },
  "bugs": {
    "url": "https://github.com/xNerhu/ftp-client#issues"
  },
  "main": "./build/main.js",
  "scripts": {
    "dev": "rimraf ./build && cross-env NODE_ENV='dev' node fuse",
    "start": "cross-env ENV='dev' electron .",
    "build": "rimraf ./build && cross-env NODE_ENV='prod' node fuse",
    "compile-win32": "npm run build && electron-builder -w -p always",
    "compile-darwin": "npm run build && electron-builder -m -p always",
    "compile-linux": "npm run build && electron-builder -l -p always",
    "lint": "tslint \"src/**/*.ts*\"",
    "lint-fix": "prettier --write \"src/**/*.ts*\" && tslint \"src/**/*.ts*\" --fix"
  },
  "devDependencies": {
    "@types/dotenv": "6.1.1",
    "@types/gsap": "1.20.2",
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.4",
    "@types/styled-components": "4.1.14",
    "cross-env": "5.2.0",
    "dotenv": "8.0.0",
    "electron": "5.0.1",
    "electron-builder": "20.40.2",
    "electron-ext-icon": "1.0.1",
    "fuse-box": "3.7.1",
    "qusly-core": "1.1.1",
    "rimraf": "2.6.3",
    "tslint": "5.17.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.3.3",
    "typescript-plugin-styled-components": "1.4.2"
  },
  "dependencies": {
    "gsap": "2.1.2",
    "mobx": "5.9.4",
    "mobx-react": "5.4.3",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "styled-components": "4.2.0"
  }
}
