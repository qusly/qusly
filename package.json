{
  "name": "qusly",
  "version": "0.1.0",
  "description": "A full-featured, elegant FTP client.",
  "keywords": [
    "typescript",
    "react",
    "styled-components",
    "electron",
    "ftp",
    "sftp",
    "client",
    "qusly"
  ],
  "author": {
    "name": "Miko≈Çaj Palkiewicz",
    "email": "xnerhu@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qusly/qusly.git"
  },
  "bugs": {
    "url": "https://github.com/qusly/qusly#issues"
  },
  "main": "./build/main.bundle.js",
  "scripts": {
    "dev": "concurrently \"cross-env ENV='dev' webpack-dev-server --config webpack.config.renderer.js\" \" cross-env ENV='dev' webpack\"",
    "build": "concurrently \"webpack --config webpack.config.renderer.js\" \"webpack\"",
    "start": "cross-env ENV='dev' electron .",
    "compile-win32": "npm run build && electron-builder -w -p always",
    "compile-darwin": "npm run build && electron-builder -m -p always",
    "compile-linux": "npm run build && electron-builder -l -p always",
    "lint": "eslint \"src/**/*.ts*\" \"src/**/*.tsx*\"",
    "lint-fix": "npm run lint -- --fix",
    "rebuild": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@types/dotenv": "8.2.0",
    "@types/gsap": "1.20.2",
    "@types/keytar": "^4.4.2",
    "@types/nedb": "^1.8.9",
    "@types/pretty-bytes": "^5.2.0",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "@types/styled-components": "4.4.1",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "awesome-node-loader": "^1.1.1",
    "cache-loader": "^4.1.0",
    "concurrently": "^5.0.2",
    "cross-env": "^6.0.3",
    "dotenv": "8.2.0",
    "electron": "7.1.7",
    "electron-builder": "21.2.0",
    "electron-ext-icon": "^1.0.1",
    "electron-updater": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "gsap": "^3.0.4",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^3.2.0",
    "mobx": "^5.15.4",
    "mobx-react-lite": "^1.5.2",
    "nedb": "^1.8.0",
    "prettier": "^1.19.1",
    "pretty-bytes": "^5.3.0",
    "qusly-core": "^2.1.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "react-windows-controls": "^1.1.1",
    "rectangle-selection": "^1.0.3",
    "resizable-box": "^1.0.0",
    "styled-components": "^4.4.1",
    "ts-loader": "^6.2.1",
    "typescript": "3.7.4",
    "typescript-plugin-styled-components": "^1.4.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "keytar": "^5.4.0"
  }
}
