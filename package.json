{
  "name": "qusly",
  "version": "0.2.1-alpha",
  "description": "A full-featured, elegant FTP client.",
  "keywords": [
    "typescript",
    "react",
    "styled-components",
    "electron",
    "ftp",
    "sftp",
    "client",
    "qusly"
  ],
  "author": {
    "name": "Miko≈Çaj Palkiewicz",
    "email": "xnerhu@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qusly/qusly.git"
  },
  "bugs": {
    "url": "https://github.com/qusly/qusly#issues"
  },
  "main": "./build/main.bundle.js",
  "scripts": {
    "dev": "concurrently \"cross-env ENV='dev' webpack-dev-server --config webpack.config.renderer.js\" \" cross-env ENV='dev' webpack\"",
    "build": "concurrently \"webpack --config webpack.config.renderer.js\" \"webpack\"",
    "start": "cross-env ENV='dev' electron .",
    "compile-win32": "npm run build && electron-builder -w -p always",
    "compile-darwin": "npm run build && electron-builder -m -p always",
    "compile-linux": "npm run build && electron-builder -l -p always",
    "lint": "eslint \"src/**/*.ts*\" \"src/**/*.tsx*\"",
    "lint-fix": "npm run lint -- --fix",
    "rebuild": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@types/dotenv": "8.2.0",
    "@types/gsap": "1.20.2",
    "@types/keytar": "^4.4.2",
    "@types/nedb": "^1.8.9",
    "@types/pretty-bytes": "^5.2.0",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "@types/styled-components": "5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "awesome-node-loader": "^1.1.1",
    "cache-loader": "^4.1.0",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "dotenv": "8.2.0",
    "electron": "18.3.7",
    "electron-builder": "22.4.1",
    "electron-ext-icon": "^1.0.1",
    "electron-updater": "^4.2.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "5.0.2",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "gsap": "^3.6.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "3.2.0",
    "mobx": "^5.15.4",
    "mobx-react-lite": "1.5.2",
    "nedb": "^1.8.0",
    "prettier": "^2.0.4",
    "pretty-bytes": "^5.3.0",
    "qusly-core": "2.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "react-windows-controls": "^1.1.1",
    "rectangle-selection": "1.0.3",
    "resizable-box": "^1.0.0",
    "styled-components": "4.4.1",
    "ts-loader": "^6.2.2",
    "typescript": "3.8.3",
    "typescript-plugin-styled-components": "^1.4.4",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "keytar": "^5.4.0"
  }
}
